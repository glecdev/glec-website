{
  "name": "glec-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:admin": "playwright test tests/e2e/admin/",
    "test:e2e:public": "playwright test tests/e2e/homepage.spec.ts tests/e2e/cloud-page.spec.ts tests/e2e/carbon-api-page.spec.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "db:migrate": "dotenv -e .env.migration -- npx prisma migrate deploy",
    "db:seed": "dotenv -e .env.migration -- npx ts-node prisma/seed.ts",
    "db:studio": "dotenv -e .env.migration -- npx prisma studio",
    "db:reset": "dotenv -e .env.migration -- npx prisma migrate reset",
    "verify:env": "node scripts/verify-vercel-env.js"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/s3-request-presigner": "^3.901.0",
    "@neondatabase/serverless": "1.0.2",
    "@prisma/client": "^5.22.0",
    "@tabler/icons-react": "^3.35.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tiptap/extension-image": "^3.6.2",
    "@tiptap/extension-link": "^3.6.2",
    "@tiptap/extension-placeholder": "^3.6.2",
    "@tiptap/pm": "^3.6.2",
    "@tiptap/react": "^3.6.2",
    "@tiptap/starter-kit": "^3.6.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "next": "^15.5.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.2.1",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "resend": "^6.1.2",
    "tailwind-merge": "^3.3.1",
    "ua-parser-js": "^2.0.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@cloudflare/next-on-pages": "^1.13.16",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.19",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.33",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "wrangler": "^4.42.0"
  }
}
